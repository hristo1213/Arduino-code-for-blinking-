Code for making an LED blink

void setup () {

pinMode(LED_BUILTIN, OUTPUT);

}

void loop () {

digitalWrite(LED_BUILTIN, HIGH);
delay(1000);
digitalWrite(LED_BUILTIN, LOW);
delay(1000);

}

Code for AnalogReadSerial 

void setup () {
Serial.begin(9600);
}

void loop () {
int sensorValue = analogRead(A0);
Serial.ptintln(sensorValue);
delay(1);
}
